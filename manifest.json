{
    "manifest_version": 3,
    "name": "0day-p0k3m0n",
    "version": "1.0",
    "description": "From 0 to Hero",
    "permissions": [
      "cookies",
      "webRequest",
      "declarativeNetRequest",
      "activeTab",
      "storage"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    }
  }
  
